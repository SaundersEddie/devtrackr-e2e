{
  "name": "devtrackr-e2e",
  "version": "1.0.0",
  "type": "module",
  "description": "Dev Tracker E2E Testing",
  "main": "index.js",
  "scripts": {
    "test:ui": "wdio run ui/wdio.conf.js",
    "test:ui:smoke": "wdio run ui/wdio.conf.js --cucumberOpts.tagExpression='@smoke'",
    "test:api": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest --runInBand --config api/jest.config.js",
    "report:ui": "multiple-cucumber-html-reporter --jsonDir=./reports/ui/json --reportPath=./reports/ui/html"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@cucumber/cucumber": "^12.2.0",
    "@wdio/cli": "^9.20.0",
    "@wdio/cucumber-framework": "^9.20.0",
    "@wdio/devtools-service": "^8.42.0",
    "@wdio/globals": "^9.17.0",
    "@wdio/local-runner": "^9.20.0",
    "@wdio/selenium-standalone-service": "^8.14.0",
    "@wdio/spec-reporter": "^9.20.0",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "expect-webdriverio": "^5.4.3",
    "jest": "^30.2.0",
    "jest-html-reporters": "^3.1.7",
    "jest-junit": "^16.0.0",
    "multiple-cucumber-html-reporter": "^3.9.3",
    "supertest": "^7.1.4",
    "webdriverio": "^9.20.0"
  }
